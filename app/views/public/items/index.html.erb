    <div class='col-md-10 offset-md-1'>
      <h3>Present List</h3>
      <table class='table'>
        <tbody>
          <% @item.each do |item| %>
            <tr>
              <td rowspan="2" scope="rowgroup"><%= image_tag item.get_image, size: '80x80', style: 'object-fit: cover;' %></td>
              <th>ユーザーネーム：</th><td><%= link_to item.user.last_name+item.user.first_name,public_user_path(item.user.id) %></td>
              <th>アイテム名：</th><td><%= link_to item.name,public_item_path(item.id) %></td>
              <th>価格：</th><td><%= item.price %></td>
            </tr>
            <tr>
              <th>コメント：</th><td><%= item.explanation %></td>
              <th>投稿日：</th><td><%= item.created_at.strftime('%Y/%m/%d') %></td>
              <!-- タグリスト -->
              <td><% item.item_tags.each do |tag| %>
              <i class="fa-sharp fa-solid fa-tag"></i>
                <%= link_to tag.name,search_tag_path(item_tag_id: tag.id) %>
              <% end %>
              </td>
              <% if item.id.nil? %>
                <div class="form-group" id="star">
                <%= f.label :star %>
                <%= f.hidden_field :star, id: :review_star, class: 'form-control' %>
                  <div id="post_raty"></div>
                </div>
                <script>
                  $(document).on('turbolinks:load', function() {
                    let elem = document.querySelector('#post_raty');
                    if (elem == null) return;
              
                    elem.innerHTML = ""
                    let opt = {  
                      starOn: "<%= asset_path('star-on.png') %>",
                      starOff: "<%= asset_path('star-off.png') %>",
                      starHalf: "<%= asset_path('star-half.png') %>",
                      scoreName: 'item[star]',
                    };
                    raty(elem, opt);
                  });
                </script>
              <% else %>
                <div class="form-group">
                  <%= render "public/items/static_rate", item: item %>
                </div>
              <% end %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
<%= paginate @items %>